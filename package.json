{
  "name": "typescript-template",
  "description": "TypeScript template for Coding AI Agent",
  "version": "1.0.0",
  "keywords": [],
  "author": "Yuku Kotani (y.kotani@dr-ubie.com)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/yukukotani/typescript-template"
  },
  "homepage": "https://github.com/yukukotani/typescript-template",
  "bugs": "https://github.com/yukukotani/typescript-template/issues",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "dist/index.js",
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint:biome": "biome check .",
    "lint:tsc": "tsc --noEmit",
    "lint:knip": "knip",
    "lint": "concurrently 'npm:lint:*'",
    "fix:biome": "biome check --write --unsafe .",
    "fix:knip": "knip --fix --allow-remove-files",
    "fix": "concurrently 'npm:fix:*'",
    "format": "biome format --write .",
    "prepublishOnly": "bun run build"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@types/node": "^24.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.0",
    "knip": "^5.62.0",
    "tsdown": "^0.14.1",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@praha/byethrow": "^0.6.3"
  }
}
